pcall(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = 'Sleezyware';
		Text = 'Brand new whitelist, loadstring and obfuscation.';
		Duration = 25;
	})
	if not game.Players.LocalPlayer:IsInGroup(12052955) then
		coroutine.resume(coroutine.create(function()
			wait(2)
			game.StarterGui:SetCore("SendNotification", {
				Title = 'Sleezyware';
				Text = 'This loadstring was discontinued, we copied the discord to your clipboard incase you're not in it. Go to #how-to-buy to learn how to whitelist yourself.';
				Duration = 25;
			})
			setclipboard('https://discord.com/sleezyware')
		end))
	end
end)
