if game.PlaceId = N/A then --game id here

    local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/Splix"))()

    local window = library:new({textsize = 13.5,font = Enum.Font.Code,name = "Sleezyware | Rooftop | .gg/sleezyshub",color = Color3.fromRGB(122, 147, 158)})

    local tab = window:page({name = "Main"})

    local section1 = tab:section({name = "Silent Aimbot Options",side = "left",size = 75})

    section1:toggle({name = "Silent Aimbot",def = false,callback = function(value)
      tog = value
      print(tog)
    end})
